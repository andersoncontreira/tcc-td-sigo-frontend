{
  "name": "tcc-td-sigo-frontend",
  "proxy": "http://localhost:5000",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "ra-data-simple-rest": "^3.10.4",
    "react": "^17.0.1",
    "react-admin": "^3.11.2",
    "react-dom": "^17.0.1",
    "react-dotenv": "^0.1.3",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-dotenv && react-scripts start",
    "build": "react-dotenv && react-scripts build",
    "test": "react-dotenv && react-scripts test",
    "eject": "react-dotenv && react-scripts eject",
    "server": "json-server --middlewares ./resources/json-server/middlewares/headers.js --watch samples/api/tcc-td-sigo-standard-manager-api/api.json --port 5000",
    "client": "npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "json-server": "^0.16.3",
    "serve": "^11.3.2"
  },
  "react-dotenv": {
    "whitelist": [
      "API_URL"
    ]
  }
}
